{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
    "require-jsdoc": 0,
    "new-cap": 0,
    "object-curly-spacing": 0,
    "quote-props": 0,
    "max-len": [
      "error",
      {
        "code": 100,
        "ignoreUrls": true
      }
    ]
  }
}

